    "build:development:device": "eas build --profile development:device --platform ios",
    "format": "pnpm dlx @biomejs/biome check --write --unsafe ./src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo .turbo node_modules"
