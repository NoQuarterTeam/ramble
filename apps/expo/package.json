{
  "name": "@ramble/expo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "APP_VARIANT=development expo start --ios --dev-client",
    "start": "APP_VARIANT=development expo start --dev-client",
    "build": "pnpm run \"/build:/\"",
    "build:production": "eas build --profile production --platform all --auto-submit",
    "build:preview": "eas build --profile preview --platform all",
    "build:development": "eas build --profile development --platform ios",
    "build:development:device": "eas build --profile development:device --platform ios",
    "format": "pnpm dlx @biomejs/biome check --apply-unsafe ./src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo .turbo node_modules"
  },
