{
  "build": {
    "development": {
      "ios": { "simulator": true },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "pnpm": "8.15.4",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "development:device": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": false },
      "pnpm": "8.15.4",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "pnpm": "8.15.4",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "production:test": {
      "channel": "production-test",
      "distribution": "store",
      "pnpm": "8.15.4",
      "env": {
        "APP_VARIANT": "development",
        "SENTRY_AUTH_TOKEN": "private"
      }
    },
    "production": {
      "channel": "production-test",
      "distribution": "store",
      "pnpm": "8.15.4",
      "env": {
        "SENTRY_AUTH_TOKEN": "private"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "releaseStatus": "draft",
        "track": "internal",
        "changesNotSentForReview": true
      }
    },
    "production:test": {
      "android": {
        "releaseStatus": "draft",
        "track": "internal",
        "changesNotSentForReview": true
      }
    }
  }
}
