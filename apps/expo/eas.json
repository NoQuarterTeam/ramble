{
  "build": {
    "development": {
      "ios": { "simulator": true },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "pnpm": "8.9.2",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "pnpm": "8.9.2",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "pnpm": "8.9.2",
      "env": {
        "SENTRY_AUTH_TOKEN": "private"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "releaseStatus": "draft",
        "track": "internal"
      }
    }
  }
}
