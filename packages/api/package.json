{
  "name": "@ramble/api",
  "version": "1.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": false,
  "scripts": {
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "lint": "eslint --config .eslintrc.js --ext .ts,.tsx src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ramble/database": "workspace:*",
    "@ramble/emails": "workspace:*",
    "@ramble/shared": "workspace:*",
    "@ramble/server-env": "workspace:*",
    "@ramble/server-services": "workspace:*",
    "@ramble/server-schemas": "workspace:*",
    "@trpc/server": "10.44.1",
    "@turf/bbox": "6.5.0",
    "@turf/center": "6.5.0",
    "@turf/helpers": "6.5.0",
    "dayjs": "1.11.10",
    "supercluster": "8.0.1",
    "superjson": "1.13.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@ramble/eslint-config": "workspace:*",
    "@types/node": "18.19.1",
    "@types/supercluster": "7.1.3",
    "dotenv-cli": "7.3.0",
    "eslint": "8.55.0",
    "prettier": "3.1.0",
    "typescript": "5.3.2"
  }
}
