{
  "name": "@ramble/api",
  "version": "1.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": false,
  "scripts": {
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "lint": "eslint --config .eslintrc.js --ext .ts,.tsx src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/lib-storage": "^3.433.0",
    "@aws-sdk/s3-request-presigner": "^3.433.0",
    "@ramble/database": "workspace:*",
    "@ramble/emails": "workspace:*",
    "@ramble/shared": "workspace:*",
    "@slack/web-api": "^6.9.1",
    "@trpc/server": "10.43.4",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "bcryptjs": "^2.4.3",
    "blurhash": "^2.0.5",
    "dayjs": "1.11.10",
    "flickr-sdk": "7.0.0-beta.6",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "6.9.7",
    "resend": "^2.0.0",
    "supercluster": "^8.0.1",
    "superjson": "1.13.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@ramble/eslint-config": "workspace:*",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "^18.18.6",
    "@types/supercluster": "^7.1.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.54.0",
    "prettier": "3.1.0",
    "typescript": "5.2.2"
  }
}
